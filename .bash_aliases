alias ll='ls -la'
# Some more alias to avoid making mistakes:
alias rm='rm -v'
alias cp='cp -v'
alias mv='mv -v'

# faster navigation
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias llw='ls --group-directories-first -lha'

# debian commands
alias architecture='dpkg --print-architecture'

# system
alias fd=fdfind